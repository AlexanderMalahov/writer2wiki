<?xml version="1.0" encoding="UTF-8"?>
<description xmlns="http://openoffice.org/extensions/description/2006"
             xmlns:descr="http://openoffice.org/extensions/description/2006"
             xmlns:xlink="http://www.w3.org/1999/xlink">

    <identifier value="com.github.teopedia.writer2wiki" />
    <version value="0.10"/>

    <dependencies>
        <!-- TODO: replace minimal version with LibreOffice 5.4(because of Python 3.5)-->
        <OpenOffice.org-minimal-version value="4.1" descr:name="OpenOffice.org 4.1"/>
    </dependencies>

    <registration>
        <simple-license accept-by="admin" default-license-id="this" suppress-on-update="true" suppress-if-required="true">
            <license-text xlink:href="LICENSE.txt" lang="en" license-id="this" />
        </simple-license>
    </registration>

    <display-name>
        <name lang="en">Writer to Wiki Converter</name>
        <name lang="ru">Конвертер из Writer в Wiki</name>
    </display-name>

</description>

