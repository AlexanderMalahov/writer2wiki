<?xml version="1.0" encoding="UTF-8"?>
<description xmlns       = "http://openoffice.org/extensions/description/2006"
             xmlns:descr = "http://openoffice.org/extensions/description/2006"
             xmlns:libre = "http://libreoffice.org/extensions/description/2011"
             xmlns:xlink = "http://www.w3.org/1999/xlink">

    <identifier value="com.github.teopedia.writer2wiki" />
    <version value="0.12-ru-en"/>

    <dependencies>
        <libre:LibreOffice-minimal-version value="5.4" descr:name="LibreOffice 5.4"/>
    </dependencies>

    <registration>
        <simple-license accept-by="admin" default-license-id="this" suppress-on-update="true" suppress-if-required="true">
            <license-text xlink:href="LICENSE.txt" lang="en" license-id="this" />
        </simple-license>
    </registration>

    <display-name>
        <name>Writer2Wiki</name>
    </display-name>

    <extension-description>
        <src lang="en" xlink:href="extension-config/ext-manager-description/descr.en.txt"/>
        <src lang="ru" xlink:href="extension-config/ext-manager-description/descr.ru.txt"/>
    </extension-description>

    <update-information>
        <src xlink:href="https://raw.githubusercontent.com/teopedia/writer2wiki/master/non-oxt-files/auto-update.xml"/>
    </update-information>

</description>

